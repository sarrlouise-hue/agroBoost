{
  "name": "agroboost-backend",
  "version": "1.0.0",
  "description": "Backend API pour l'application AGRO BOOST - Plateforme de réservation de services agricoles",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "npm install",
    "vercel-build": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:integration:real": "jest --testMatch='**/tests/integration/*.integration.test.js'",
    "init-db": "node scripts/init-db.js",
    "init-db:force": "node -e \"require('dotenv').config(); const {sequelize} = require('./src/config/database'); require('./src/models/User'); require('./src/models/OTP'); require('./src/models/PasswordResetToken'); require('./src/models/Provider'); require('./src/models/Service'); require('./src/models/Booking'); require('./src/models/Payment'); require('./src/models/associations'); sequelize.sync({force: true}).then(() => {console.log('✅ Tables recréées'); process.exit(0);}).catch(e => {console.error('❌ Erreur:', e); process.exit(1);});\"",
    "migrate:otp-email": "node migrations/20250101-change-otp-phone-to-email.js",
    "migrate:maintenances": "node migrations/20250102-create-maintenances-table.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "agriculture",
    "senegal",
    "api",
    "mobile-money",
    "booking"
  ],
  "author": "AGRO BOOST",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "geolib": "^3.3.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-geocoder": "^4.2.0",
    "nodemailer": "^7.0.11",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.11",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
