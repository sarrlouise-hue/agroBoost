    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Sauvegarder automatiquement les tokens après connexion",
          "if (pm.response.code === 200 || pm.response.code === 201) {",
          "    const responseJson = pm.response.json();",
          "    if (responseJson.data) {",
          "        if (responseJson.data.token) {",
          "            pm.collectionVariables.set('access_token', responseJson.data.token);",
          "        }",
          "        if (responseJson.data.refreshToken) {",
          "            pm.collectionVariables.set('refresh_token', responseJson.data.refreshToken);",
          "        }",
          "        // Sauvegarder les IDs depuis les réponses",
          "        if (responseJson.data.id) {",
          "            // Si c'est un provider",
          "            if (responseJson.data.businessName) {",
          "                pm.collectionVariables.set('provider_id', responseJson.data.id);",
          "            }",
          "            // Si c'est un service",
          "            if (responseJson.data.serviceType) {",
          "                pm.collectionVariables.set('service_id', responseJson.data.id);",
          "            }",
          "            // Si c'est un user",
          "            if (responseJson.data.phoneNumber && !responseJson.data.businessName) {",
          "                pm.collectionVariables.set('user_id', responseJson.data.id);",
          "            }",
          "        }",
          "    }",
          "    // Sauvegarder le reset token depuis forgot-password",
          "    if (responseJson.data && responseJson.data.resetToken) {",
          "        pm.collectionVariables.set('reset_token', responseJson.data.resetToken);",
          "    }",
          "}"
        ]
      }
    }
  ]
